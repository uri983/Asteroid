<?xml version='1.0' encoding='utf-8'?>
<widget id="com.acdqrtimechecker.app" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TIME CHECKER</name>
    <description>
        Aplicacion de Check in / Check out
    </description>
    <author email="velasquez_uriel@hotmail.com" href="http://cordova.io">
        Uriel Velasquez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="portrait" />
</widget>
